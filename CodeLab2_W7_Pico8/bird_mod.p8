function menu_update()
	--move the cloud to the left
	for c in all(clouds) do
		c.x-=cloud_v
		
		if(c.x<-128)then
			temp=c
			c.x=clouds[cloud_num].x+128
			del(clouds,c)
			add(clouds,temp)
		end
	
	end
	
	--move the hill to the left
	for h in all(hills) do
		h.x-=hill_v
		
		if(h.x<-128)then
			temp=h
			h.x=hills[hill_num].x+128
			del(hills,h)
			add(hills,temp)
		end
	
	end
	
	--move the ground to the left
	for g in all(ground) do
		
		g.x-=ground_v
		
		if(g.x<-32)then
			temp=g
			g.x=ground[ground_num+1].x+32
			del(ground,g)
			add(ground,temp)
		end
	
	end

	bounce_title()
	
	--if press the x button, start the game
	if(btn(5,0)) state="game"
	
end

function menu_draw()

	--background color: blue
	cls(12)
	
	--draw the clouds
	for c in all(clouds) do
		spr(64,c.x,c.y,16,4)
	end
	
	--draw the hills
	for h in all(hills) do
		spr(128,h.x,h.y,16,4)
	end
	
	--draw the ground
	for g in all(ground) do
		spr(8,g.x,g.y,4,4)
	end
	
	--draw the title
	spr(192,28,title_y,9,2)
	
	--draw the text
	print("press x to start",32,50,7)
	print("z to flap",47,60,0)

end

--title animation
function bounce_title()
	
	title_y=lerp(title_y,title_tar,0.9)
	
	if(title_y>=title_max-0.5)then
		title_tar=title_min
	end
	
	if(title_y<=title_min+0.5)then
		title_tar=title_max
	end

end
