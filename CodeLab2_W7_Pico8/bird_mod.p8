--game state

function game_update()
	
	--if bird hit the tube, play a sound, and switch to end state
	--mod: if bird is not in the powerup state
	for t in all(tubes) do
		if(overlap(t) and overlapPowerup==false)then
			sfx(1)
			state="end"		
		end
		
		--move the tube to left
		t.x-=tube_v
		
		--if tube moves out of boundary, delete the tube (?32)
		--32 is the width of the tube
		if(t.x<-32)then
			temp=t
			--move the tube to the right
			t.x=tubes[tube_num+1].x+offset
			
			--??? maybe relates to variable address
			--remove the old tube and add the new tube
			del(tubes,t)
			add(tubes,temp)
		end
		
		--if the bird pass the tube, add one score and play sound
		if(bird_x==t.x)then
			sfx(3)
			score+=1
		end

		--BUG FIX: add ground/sky collision
		if(bird_y+8>=112) then
			sfx(1)
			state="end"
		end

		if(bird_y<=0) then
			sfx(1)
			state="end"
		end
	end
	
	--Move the ground to left
	for g in all(ground) do
		g.x-=ground_v
		
		if(g.x<-32)then
			temp=g
			g.x=ground[ground_num+1].x+32
			del(ground,g)
			add(ground,temp)
		end
	
	end
	
	--Move the cloud to the left
	for c in all(clouds) do
		c.x-=cloud_v
		
		if(c.x<-128)then
			temp=c
			c.x=clouds[cloud_num].x+128
			del(clouds,c)
			add(clouds,temp)
		end
	
	end
		
	--Move the hill to the left
	for h in all(hills) do
		h.x-=hill_v
		
		if(h.x<-128)then
			temp=h
			h.x=hills[hill_num].x+128
			del(hills,h)
			add(hills,temp)
		end
	
	end

	bird_move()		

if(overlapPowerup == false and tubeIsOnZero == false)then
	--mod:move powerup 
	powerup_move()

	--powerup ani
	if(frame<3)then
		frame +=1
	elseif(frame ==3)then
		powerup_ani()
		frame=0		
	end	
end

	--check if the bird overlap with the powerup
	overlapPower()
end

--draw the game scene
function game_draw()
	
	--background color
	cls(12)
		
	for c in all(clouds) do
		spr(64,c.x,c.y,16,4)
	end
		
	for h in all(hills) do
		spr(128,h.x,h.y,16,4)
	end
		
	for t in all(tubes) do
		draw_tube(t)
	end
		
	for g in all(ground) do
		spr(8,g.x,g.y,4,4)
	end
		
	spr(bird_spr,bird_x,bird_y,2,2)

	--mod: draw the powerup
	spr(powerup_spr,powerup_x,powerup_y,1,2)

	rectfill(64,10,66,14,7)
	print(score,64,10,0)

end

--mod:move powerup from right to left
function powerup_move()
	powerup_x-=powerup_v	
	if(powerup_x<=0)then
		powerup_x = 128
	end
end

function spawnPowerup()
    
	powerup_x=128
	powerup_y=30+rnd(60)

end

--mod:update powerup animation 
function powerup_ani()
	if(powerup_spr<207)then
		powerup_spr+=1
	elseif(powerup_spr==207)then
		powerup_spr=201
	end
end

function bird_move()
	
	--add dropping velocity
	bird_v+=gravity
	--add velocity to y axis
	bird_y+=bird_v
	
	--if player press the z button
	if(btn(4,0) and not pressed)then
		--play the sound
		sfx(0)
		--decrease the dropping velocity
		bird_v-=jump_force
		--change the bird sprite to flying state
		bird_spr=14
		pressed=true
	end
	
	-- if player do not press the z button
	if(not btn(4,0))then
		--change the bird sprite
		bird_spr=12
		pressed=false
	end

end
